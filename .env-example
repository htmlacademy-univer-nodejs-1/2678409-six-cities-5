# Порт сервера
PORT=3000

# IP адрес сервера баз данных MongoDB
DB_HOST=127.0.0.1

# Порт MongoDB
DB_PORT=27017

# Имя базы данных
DB_NAME=six-cities

# Пользователь MongoDB (опционально, если не используется аутентификация)
# DB_USER=

# Пароль MongoDB (опционально, если не используется аутентификация)
# DB_PASSWORD=

# Соль для хеширования (замените на случайную строку)
SALT=your_random_salt_string_here

# Директория для загрузок пользовательских файлов
UPLOAD_DIR=uploads

# Директория для статических файлов (стили, скрипты и т.д.)
STATIC_DIR=public

# Секрет для JWT токенов
JWT_SECRET=your_jwt_secret_here
